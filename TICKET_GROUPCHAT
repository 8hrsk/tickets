let ddi; // localStorage key

async function creategroupthread() { // создаем групповой чат
  fetch("https://ficbook.net/home/messaging/creategroupthread", {
    "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
    "content-type": "application/json",
    "sec-ch-ua": "\"Chromium\";v=\"110\", \"Not A(Brand\";v=\"24\", \"Google Chrome\";v=\"110\"",
    "sec-ch-ua-mobile": "?0",
    "sec-ch-ua-platform": "\"Windows\"",
    "sec-fetch-dest": "empty",
    "sec-fetch-mode": "cors",
    "sec-fetch-site": "same-origin"
    },
    "referrer": "https://ficbook.net/home/messaging",
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": "{}",
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
    }
  )
  .then(Response => {
    return Response.json(); // получаем ответ
  })

  .then((data) => {
    //var id_from_data = Array.from(data.data.id); //достаем id из ответа
    //let threadid = id_from_data.reduce((acc, id_from_data) => acc + id_from_data); // убираем символьное разделение
    //t_id += String(threadid); // пихаем в массив
    ////console.log(String(threadid));
    //let id_data = String(data.data.id);
    //console.log(id_data);
    //globalVar.push(id_data);
    //console.log(globalVar);
    localStorage.setItem(ddi, data.data.id); // save data to localStorage
  });
}

async function adduser(t_id, username) {
  fetch("https://ficbook.net/home/messaging/addusertothread", {
    "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
    "content-type": "application/json",
    "sec-ch-ua": "\"Chromium\";v=\"110\", \"Not A(Brand\";v=\"24\", \"Google Chrome\";v=\"110\"",
    "sec-ch-ua-mobile": "?0",
    "sec-ch-ua-platform": "\"Windows\"",
    "sec-fetch-dest": "empty",
    "sec-fetch-mode": "cors",
    "sec-fetch-site": "same-origin"
    },
    "referrer": "https://ficbook.net/home/messaging",
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": "{\"thread_id\":\" " + t_id + "\",\"username\":\"" + username + "\"}",
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
    }
  );
}

async function leave(t_id) {
  fetch("https://ficbook.net/home/messaging/leavethread", {
    "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
    "content-type": "application/json",
    "sec-ch-ua": "\"Chromium\";v=\"110\", \"Not A(Brand\";v=\"24\", \"Google Chrome\";v=\"110\"",
    "sec-ch-ua-mobile": "?0",
    "sec-ch-ua-platform": "\"Windows\"",
    "sec-fetch-dest": "empty",
    "sec-fetch-mode": "cors",
    "sec-fetch-site": "same-origin"
    },
    "referrer": "https://ficbook.net/home/messaging",
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": "{\"thread_id\":\" " + t_id + "\"}",
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
    }
  );
}

async function send(t_id) {
  fetch("https://ficbook.net/home/messaging/send", {
    "headers": {
    "accept": "application/json, text/plain, */*",
    "accept-language": "ru-RU,ru;q=0.9,en-US;q=0.8,en;q=0.7",
    "content-type": "application/json",
    "sec-ch-ua": "\"Chromium\";v=\"110\", \"Not A(Brand\";v=\"24\", \"Google Chrome\";v=\"110\"",
    "sec-ch-ua-mobile": "?0",
    "sec-ch-ua-platform": "\"Windows\"",
    "sec-fetch-dest": "empty",
    "sec-fetch-mode": "cors",
    "sec-fetch-site": "same-origin"
    },
    "referrer": "https://ficbook.net/home/messaging",
    "referrerPolicy": "strict-origin-when-cross-origin",
    "body": "{\"thread_id\":\" " + t_id + "\",\"text\":\"t.me/ehrsk\"}",
    "method": "POST",
    "mode": "cors",
    "credentials": "include"
    }
  );
}

function sleep(ms) {
  return new Promise(resolve => setTimeout(resolve, ms));
}

while (true) {

    creategroupthread();
    let t_id = localStorage.getItem(ddi);
    await sleep(100);

    var username = usernames[Math.floor(Math.random()*usernames.length)];
    adduser(t_id, username);
    await sleep(60);
    var username = usernames[Math.floor(Math.random()*usernames.length)];
    adduser(t_id, username);
    await sleep(60);
    var username = usernames[Math.floor(Math.random()*usernames.length)];
    adduser(t_id, username);
    await sleep(60);
    var username = usernames[Math.floor(Math.random()*usernames.length)];
    adduser(t_id, username);
    await sleep(60);
    
    console.log('added' + username);
    await sleep (100);
    send(t_id);
    console.log('send');
}
